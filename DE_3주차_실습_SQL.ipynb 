              "            <th>left</th>\n",
              "            <th>case</th>\n",
              "        </tr>\n",
              "    </thead>\n",
              "    <tbody>\n",
              "        <tr>\n",
              "            <td>2019-08</td>\n",
              "            <td>&gt;= 10,000</td>\n",
              "        </tr>\n",
              "        <tr>\n",
              "            <td>2019-11</td>\n",
              "            <td>&gt;= 10,000</td>\n",
              "        </tr>\n",
              "        <tr>\n",
              "            <td>2019-05</td>\n",
              "            <td>&lt; 10,000</td>\n",
              "        </tr>\n",
              "        <tr>\n",
              "            <td>2019-06</td>\n",
              "            <td>&gt;= 10,000</td>\n",
              "        </tr>\n",
              "        <tr>\n",
              "            <td>2019-07</td>\n",
              "            <td>&gt;= 10,000</td>\n",
              "        </tr>\n",
