      "source": [
        "월별로 10000 혹은 그 이상 세션이면 \">= 10,000\" 아니면 \"< 10,000\" 이렇게 찍어보기"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "99vUfoOBtMCn",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 221
        },
        "outputId": "29342a7d-61b8-42e8-a3b0-9be552f5ee84"
      },
      "source": [
        "%%sql\n",
        "\n",
        "SELECT LEFT(ts, 7), CASE WHEN COUNT(1) >= 10000 THEN '>= 10,000' ELSE '< 10,000' END\n",
        "FROM raw_data.session_timestamp\n",
        "GROUP BY 1;"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            " * postgresql://cing93:***@learnde.cduaw970ssvt.ap-northeast-2.redshift.amazonaws.com:5439/dev\n",
            "7 rows affected.\n"
          ],
          "name": "stdout"
        },
        {
          "output_type": "execute_result",
          "data": {
            "text/html": [
              "<table>\n",
