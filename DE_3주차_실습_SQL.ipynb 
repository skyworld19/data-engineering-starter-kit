      "source": [
        "### 요약 테이블을 CTAS (Create Table 이름 As SELECT)로 생성해보기"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "WdIU2WEiB5HJ",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "e5a009ea-6b81-46f9-8c77-47eca65ea48d"
      },
      "source": [
        "%%sql\n",
        "\n",
        "DROP TABLE IF EXISTS cing93.monthly_active_user_summary;\n",
        "CREATE TABLE cing93.monthly_active_user_summary AS \n",
        "SELECT TO_CHAR(ts, 'YYYY-MM') AS month, COUNT(DISTINCT userid)\n",
        "FROM raw_data.user_session_channel A\n",
        "JOIN raw_data.session_timestamp B ON A.sessionid = B.sessionid\n",
        "GROUP BY 1\n",
        "ORDER BY 1 DESC;"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            " * postgresql://cing93:***@learnde.cduaw970ssvt.ap-northeast-2.redshift.amazonaws.com:5439/dev\n",
            "Done.\n",
            "Done.\n"
          ],
          "name": "stdout"
