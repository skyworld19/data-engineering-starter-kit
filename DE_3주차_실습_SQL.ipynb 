      },
      "source": [
        "%%sql\n",
        "\n",
        "SELECT ts, channel\n",
        "FROM raw_data.user_session_channel usc\n",
        "JOIN raw_data.session_timestamp st ON usc.sessionid = st.sessionid\n",
        "WHERE userid = 251\n",
        "ORDER BY 1;"
      ],
      "execution_count": null,
      "outputs": []
