      "source": [
        "### UNION과 UNION ALL 차이점 이해하기"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "7DSKD3PslAGZ",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 116
        },
        "outputId": "ef2bff0c-d2d8-4fe2-cb44-a419f5157d06"
      },
      "source": [
        "%%sql\n",
        "--UNION 은 중복제거, UNOIN ALL은 중복제거를 하지 않음\n",
        "SELECT 'keeyong' as first_name, 'han' as last_name\n",
        "\n",
        "UNION\n",
        "\n",
        "SELECT 'elon', 'musk'\n",
        "\n",
        "UNION\n",
        "\n",
        "SELECT 'keeyong', 'han'"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            " * postgresql://cing93:***@learnde.cduaw970ssvt.ap-northeast-2.redshift.amazonaws.com:5439/dev\n",
            "2 rows affected.\n"
          ],
          "name": "stdout"
        },
        {
          "output_type": "execute_result",
          "data": {
            "text/html": [
              "<table>\n",
              "    <thead>\n",
              "        <tr>\n",
              "            <th>first_name</th>\n",
              "            <th>last_name</th>\n",
              "        </tr>\n",
              "    </thead>\n",
              "    <tbody>\n",
              "        <tr>\n",
              "            <td>elon</td>\n",
              "            <td>musk</td>\n",
              "        </tr>\n",
              "        <tr>\n",
              "            <td>keeyong</td>\n",
